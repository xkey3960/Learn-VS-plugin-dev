{
  "name": "learning-tree-view",
  "displayName": "learning-tree-view",
  "description": "Second plugin for learning.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [{
      "command": "projMnger.editEntry",
      "title": "Edit",
      "icon": {
        "light": "resources/light/edit.svg",
        "dark": "resources/dark/edit.svg"
      }
    },
    {
      "command": "projMnger.refreshEntry",
      "title": "Refresh",
      "icon": {
        "light": "resources/light/refresh.svg",
        "dark": "resources/dark/refresh.svg"
      }
    }],
    "views": {
      "explorer": [
        {
          "id": "projMnger",
          "name": "Tasks"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "projMnger.refreshEntry",
          "when": "view == projMnger",
          "group": "navigation"
        }
      ],
      "view/item/context": [
        {
          "command": "projMnger.editEntry",
          "when": "view == projMnger",
          "group": "inline"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.90.0",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0"
  }
}
